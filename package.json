{
  "name": "@pittankopta/vibranium",
  "version": "1.0.1",
  "description": "CLI for managing custom emulated devices on Chromium-based browsers.",
  "author": "Pittan @Pittan",
  "bin": {
    "vibranium": "./bin/run"
  },
  "bugs": "https://github.com/Pittan/vibranium/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chrome-profile-list": "^1.1.0",
    "find-process": "^1.4.3",
    "inquirer": "^7.1.0",
    "json-beautify": "^1.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "3",
    "chai": "^4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "globby": "^10",
    "mocha": "8",
    "nyc": "^14",
    "standard-version": "^9.3.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Pittan/vibranium",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vibranium",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Pittan/vibranium",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "release": "standard-version"
  },
  "types": "lib/index.d.ts"
}
