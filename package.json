{
  "name": "@pittankopta/vibranium",
  "description": "CLI for managing custom emulated devices on Chromium-based browsers.",
  "version": "2.0.0-alpha.0",
  "author": "Pittan @Pittan",
  "bin": {
    "vibranium": "./bin/run.js"
  },
  "bugs": "https://github.com/Pittan/vibranium/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.8.4",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "find-process": "^2.0.0",
    "json-beautify": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/Pittan/vibranium",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "vibranium",
    "dirname": "vibranium",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "Pittan/vibranium",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
